@model MvcCV.Models.Entity.Tbl_Sertifika
@{
    ViewBag.Title = "SertifikaGetir";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}




<br />
<h4>Sertifika Düzenleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
    <a href="/Sertifika/SertifikaSil/@ViewBag.d"class="btn btn-danger">Sil</a>
    
</form>



